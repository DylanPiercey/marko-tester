{
  "name": "marko-tester",
  "version": "8.9.5",
  "description": "Assist Marko components unit/client testing",
  "repository": {
    "type": "git",
    "url": "git@github.com:oxala/marko-tester.git"
  },
  "main": "./jest/src",
  "bin": {
    "markotester": "./source/markotester.js"
  },
  "scripts": {
    "postinstall": "git submodule update --init --remote",
    "lint": "yarn test --no-mocha",
    "test": "node ./source/markotester source --no-coverage",
    "test:integration": "node ./source/markotester test",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha $(find source -name '*.spec.js')",
    "clean": "rm -rf .beans .cache static source/browser.json $(find test -name '*.marko.js') $(find source -name '*.marko.js') .coverage",
    "bump": "npm_bump --auto --auto-fallback minor",
    "prepush": "yarn lint && yarn test && yarn test:integration"
  },
  "prepush": [
    "test"
  ],
  "keywords": [
    "marko",
    "test",
    "marko-tester",
    "coverage",
    "eslint",
    "lint"
  ],
  "author": "Ryshkov, Gleb <gleb@ryshkov.com>",
  "license": "ISC",
  "dependencies": {
    "@lasso/marko-taglib": "^1.0.10",
    "app-module-path": "^2.0.0",
    "async": "^2.1.2",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.3.0",
    "eslint-config-marko-tester": "^1.2.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "html-normalizer": "https://github.com/oxala/normalizer.git",
    "istanbul": "^0.4.5",
    "istanbul-lib-instrument": "^1.10.1",
    "jsdom": "^11.1.0",
    "just-clone": "^3.0.0",
    "lasso": "^3.1.3",
    "lasso-less": "^3.0.1",
    "lasso-marko": "^2.4.0",
    "lodash": "^4.13.1",
    "marko": "^4",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.15.0",
    "mocha-multi-reporters": "^1.1.6",
    "mock-require": "^1.3.0",
    "optimist": "^0.6.1",
    "rewire": "^3.0.2",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "stack-trace": "0.0.9",
    "stream-browserify": "^2.0.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "try-require": "^1.2.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "release-tools": "^2.7.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
